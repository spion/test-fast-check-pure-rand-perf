{
  "name": "fastcheck-perf-purerand",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "show-pure-rand": "((test -e node_modules/fast-check/node_modules/pure-rand/package.json && cat node_modules/fast-check/node_modules/pure-rand/package.json) || cat node_modules/pure-rand/package.json) | grep version",
    "start": "tsc && yarn add pure-rand@1.1.1 && yarn show-pure-rand && node src/index && yarn add pure-rand@1.5.0 && yarn show-pure-rand && node src/index"
  },
  "devDependencies": {
    "ts-node": "^7.0.1",
    "typescript": "^3.1.3"
  },
  "dependencies": {
    "fast-check": "^1.7.0",
    "pure-rand": "1.5.0"
  }
}
